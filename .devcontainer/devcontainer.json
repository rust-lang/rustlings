{
  "image": "mcr.microsoft.com/vscode/devcontainers/rust",
  "runArgs": ["--cap-add=SYS_PTRACE", "--security-opt", "seccomp=unconfined"],
  "customizations": {
    "vscode": {
      "extensions": [
        "vadimcn.vscode-lldb",
        "rust-lang.rust-analyzer"
      ]
    }
  },
  "postCreateCommand": "cargo install --force --path ."
}
